<h1>Tor Wiki</h1>
<div class="wrapper">
  <div class="container">
    <div class="row">
      <% @articles.each do |article|  %>
        <div class="col-lg-3">
          <div class="card">
            <h3><%= link_to article.title, article_path(article) %></h3>
          </div>
        </div>
      <% end %>
    </div>
    <p>Find me in app/views/articles/index.html.erb</p>
  </div>
</div>
